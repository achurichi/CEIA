    "import serial\n",
    "import time\n",
    "\n",
    "PORT = \"/dev/ttyUSB0\"\n",
    "BAUD_RATE = 115200\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "serial_port = serial.Serial(PORT, BAUD_RATE, timeout=0.25)\n",
    "serial_port.flushInput()\n",
    "\n",
    "for i in range(10):\n",
    "    serial_port.write(str.encode(f\"Test {i + 1}\\n\"))\n",
    "    time.sleep(1)\n",
    "\n",
    "serial_port.close()"
   ]
  }
